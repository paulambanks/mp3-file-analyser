{
  "name": "mp3-file-analyser",
  "author": "Paula Banks",
  "license": "UNLICENSED",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">= 20"
  },
  "description": "App that accepts an MP3 file and responds with the number of frames in the file.",
  "main": "./main.ts",
  "scripts": {
    "start": "node --import=tsx main.ts",
    "start:watch": "node --import=tsx --watch main.ts",
    "build": "tsc",
    "build:watch": "tsc --watch --watchDirectory usefsevents --watchFile usefsevents --preserveWatchOutput",
    "test": "tsx --test **/*.test.ts"
  },
  "devDependencies": {
    "@pnb/eslint": "^2.2.0",
    "@types/chai": "^4.3.11",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.8",
    "@types/supertest": "^6.0.2",
    "chai": "^5.0.0",
    "eslint": "^8.56.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "supertest": "^6.3.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "multer": "^1.4.5-lts.1"
  }
}
